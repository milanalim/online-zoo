{
  "name": "online-zoo",
  "version": "1.0.0",
  "description": "online-zoo",
  "main": "index.js",
  "scripts": {

    "watch:sass": "node-sass styles/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milanalim/online-zoo.git"
  },
  "author": "Lena",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milanalim/online-zoo/issues"
  },
  "homepage": "https://github.com/milanalim/online-zoo#readme",
  "devDependencies": {
    "node-sass": "^7.0.1"
  },
  "dependencies": {
    "autoprefixer": "^10.4.8",
    "concat": "^1.0.3",
    "livereload": "^0.9.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0"
  }
}
